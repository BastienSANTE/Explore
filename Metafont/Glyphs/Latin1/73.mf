input ../Config/ExploreSansCFG.mf;

export_setup;

% U+0049 LATIN CAPITAL LETTER I - CODE 73
newchar(73, 2.3pt#, 0);
  x1 = 0.5w; y1 = CAP_H;
  x2l = x1l; y2 = BASELINE;

  penpos1(STEM, 0); penpos2(STEM, 0);

  if MONOSPACE = true:
    z3r = (CURVE_BEARING, y1); z4r = (w - CURVE_BEARING, y1);
    z5r = (CURVE_BEARING, y2); z6r = (w - CURVE_BEARING, y2);

    penpos3(BAR, 90); penpos4(BAR, 90);
    penpos5(BAR, -90); penpos6(BAR, -90);

    fill z3r -- z4r -- z4l -- z3l -- cycle;
    fill z5r -- z6r -- z6l -- z5l -- cycle;
  fi

  if SERIF = true:
    makeserif(1, z1l, -SERIF_LENGTH, -SERIF_THICKNESS);
    makeserif(2, z1r,  SERIF_LENGTH, -SERIF_THICKNESS);
    makeserif(3, z2l, -SERIF_LENGTH,  SERIF_THICKNESS);
    makeserif(4, z2r,  SERIF_LENGTH,  SERIF_THICKNESS);
  fi

  penstroke z1e -- z2e;
  italicize;
  penlabels(range 1 thru 2);
endchar;
end
