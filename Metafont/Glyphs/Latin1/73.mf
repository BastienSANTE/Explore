outputtemplate := "%c.svg";
outputformat := "svg";

PROP_WIDTH:= 2.3pt#;

input ../Config/ExploreSansCFG.mf;

% U+0049 LATIN CAPITAL LETTER I - CODE 73
beginchar(73, WIDTH, EM_H, DEPTH);
  x1 = 0.5w; y1 = CAP_H;
  x2l = x1l; y2 = BASELINE;

  penpos1(mediumy, 0); penpos2(mediumy, 0);
  penstroke z1e -- z2e;
  penlabels(range 1 thru 2);
endchar;
end
