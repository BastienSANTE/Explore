% U+0056 LATIN CAPITAL LETTER V - CODE 86
if MONOSPACE = true: WIDTH:= MONO_WIDTH; else: WIDTH:= STRETCH * 6.55pt#; fi
beginchar(86, WIDTH, EM_H, DEPTH)
  x1l = LINE_BEARING / 2; y1 = CAP_H;
  x2 = 0.5w; y2 = BASELINE;
  x3r = w - (LINE_BEARING / 2); y3 = y1;

  penpos1(mediumy, 0); penpos2(mediumy, 0); penpos3(mediumy, 0);

  penstroke z1e -- z2e; penstroke z3e -- z2e;
  penlabels(range 1 thru 3);
endchar;

