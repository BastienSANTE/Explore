input ../Config/ExploreSansCFG.mf;

export_setup;

% U+005B LEFT SQUARE BRACKET - CODE 91
newchar(91, 4pt#, 1);
if MONOSPACE = true:
  z1l = (0.25w, ASCENDER);
  z4r = (0.85w, y1l);
else:
  z1r = (LINE_BEARING, ASCENDER);
  z4r = (w - LINE_BEARING, y1l);
fi

z2l = (x1l, LOWPOINT);
z3r = z1r;
z5l = z2r; z6l = (x4r, y2l);

penpos1(STEM, 0); penpos2(STEM, 0);

forsuffixes $ = 3, 4, 5, 6:
  penpos$(BAR, 90);
endfor;

penstroke z1e -- z2e; penstroke z3e -- z4e; penstroke z5e -- z6e;
penlabels(range 1 thru 6);

italicize;

endchar;
end
