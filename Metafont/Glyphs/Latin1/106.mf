input ../Config/ExploreSansCFG.mf;

export_setup;

% U+006A LATIN SMALL LETTER J | CODE 106
newchar(106, 2.7pt#, 0);
  left_jut:= 0.2pt;

  z1 = (.5w, X_H);
  z2 = (x1, -.2pt);
  z3r = (0 + left_jut, DESCENDER);
  z4r = (0, y3r);

  z5 = (x1, X_H + ACC_YOFFSET);


  penpos1(STEM, 0); penpos2(STEM, 0); penpos3(BAR, -90); penpos4(BAR, -90); 

  penstroke z1e -- z2e{down}..tsn..z3e{left} -- z4e;

  fill fullcircle scaled DOTSIZE shifted z5;

endchar;
end
