% U+0050 LATIN CAPITAL LETTER P - CODE 80
if MONOSPACE = true: WIDTH:= MONO_WIDTH; else: WIDTH:= STRETCH * 6pt#; fi
beginchar(80, WIDTH, EM_H, DEPTH);

  x1l = LINE_BEARING; y1 = CAP_H;
  x2l = x1l; y2 = BASELINE;

  x3 = x1; y3r = CAP_H;
  x4 = 0.55w; y4r = y3r;
  x5r = w - CURVE_BEARING; y5 = 0.7 * CAP_H;
  x6 = x4; y6r = 0.4 * CAP_H;
  x7 = x1; y7r = y6r;
  
  penpos1(mediumy, 0); penpos2(mediumy, 0);
  penpos3(mediumx, 90); penpos4(mediumx, 90); penpos5(mediumy, 0); penpos6(mediumx, -90); penpos7(mediumx, -90);

  penstroke z1e -- z2e;
  penstroke z3e -- z4e{right}..z5e{down}..z6e{left} -- z7e;
  
  penlabels(range 0 thru 99);
endchar;

