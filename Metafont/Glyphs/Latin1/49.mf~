  export_setup;

% U+0031 DIGIT ONE - CODE 49
newchar(49, 5.5pt#, 0);
z1 = (0.534w, CAP_H);
z2 = (x1, BASELINE);

z3r = (0.13w, BASELINE);
z4r = (0.9w, y3r);

z5r = (x3, 0.81 * CAP_H);
z11r = z1l;

penpos1(STEM, 0); penpos2(STEM, 0);
penpos3(BAR, 90); penpos4(BAR, 90);
penpos5(BAR, 0); penpos11(BAR, 0);
endchar;
end
