input ../Config/ExploreSansCFG.mf

  export_setup;

% U+0022 QUOTATION MARK - CODE 34
newchar(34, 4pt#, 0);
z1l = (lerpbold(LINE_BEARING, OBLIQUE_BEARING), ASCENDER);
z2l = (x1l, CAP_H - 1pt);

help1 = (mid, lerp(y1, y2, 0.5));

penpos1(STEM, 0); penpos2(STEM, 0);

elem1 = z1l -- z1r -- z2r -- z2l -- cycle;

elem2 = elem1 rotatedaround(help1, 180);

fill elem1; fill elem2;

italicize;

penlabels(range 1 thru 2);

endchar;
end
